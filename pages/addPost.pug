extends head 
block content
    div.container.box-add-post
        h2 Add new post
        form#add-post
            input(type="text", name="titel" placeholder='titel' required)
            textarea(type="text", name="description" placeholder='description' required)
            input(type="file", name="file"  required)
            input(type="submit",class="btn btnAddPost", value="send")